{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"]
    }],
    "react/jsx-no-bind": [1, {
      "ignoreDOMComponents": false
    }]
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
